<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp"
    android:background="@color/Black"
    android:id="@+id/Levels"
    tools:context=".Levels">



    <Button
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:id="@+id/Home"
        android:onClick="Home"
        android:background="@drawable/homedark"
        />
    <ImageView
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:src="@drawable/gold"
        app:layout_constraintLeft_toRightOf="@id/Home"
        android:layout_marginLeft="80dp"
        android:id="@+id/coin"
        />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@id/coin"
        android:id="@+id/Money"
        android:text="10"
        android:textSize="20dp"
        android:layout_marginLeft="20dp"
        android:textColor="@color/White"
        />


    <Button
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/b1"
        android:text="1"
        android:textSize="20dp"
        android:onClick="Button1"
        android:layout_marginTop="70dp"
        app:layout_constraintTop_toBottomOf="@id/Home"

        />


    <Button
        android:clickable="false"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/Home"
        android:layout_marginTop="70dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:id="@+id/b2"
        android:textSize="20dp"
        android:text="2"
        android:onClick="Button2"
        />
    <Button
        android:clickable="false"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="70dp"
        android:id="@+id/b3"
        android:textSize="20dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/Home"
        android:text="3"
        android:onClick="Button3"
        />
    <Button
        android:clickable="false"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/b1"
        android:layout_marginTop="70dp"
        android:id="@+id/b4"
        android:textSize="20dp"
        android:text="4"
        android:onClick="Button4"
        />
    <Button
        android:clickable="false"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/b2"
        android:layout_marginTop="70dp"
        android:textSize="20dp"
        android:id="@+id/b5"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:text="5"
        android:onClick="Button5"
        />
    <Button
        android:clickable="false"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/b1"
        android:layout_marginTop="70dp"
        android:id="@+id/b6"
        android:textSize="20dp"
        app:layout_constraintRight_toRightOf="parent"
        android:text="6"
        android:onClick="Button6"
        />
    <Button
        android:clickable="false"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/b6"
        android:layout_marginTop="70dp"
        android:id="@+id/b7"
        android:text="7"
        android:textSize="20dp"
        android:onClick="Button7"
        />
    <Button
        android:clickable="false"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/b5"
        android:layout_marginTop="70dp"
        android:id="@+id/b8"
        android:textSize="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:text="8"
        android:onClick="Button8"
        />
    <Button
        android:clickable="false"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/b6"
        android:layout_marginTop="70dp"
        app:layout_constraintRight_toRightOf="parent"
        android:id="@+id/b9"
        android:text="9"
        android:textSize="20dp"
        android:onClick="Button9"
        />
    <Button
        android:clickable="false"
        android:layout_width="50dp"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@id/b8"
        android:layout_marginTop="70dp"
        android:id="@+id/b10"
        android:textSize="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:text="10"
        android:onClick="Button10"
        android:layout_marginBottom="30dp"
        />
</androidx.constraintlayout.widget.ConstraintLayout>